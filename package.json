{
  "name": "svenjungnickel",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint:js": "eslint --ext \".js,.vue,.ts\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check ./*/**.js",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lintfix": "prettier --write --list-different ./*/**.js && npm run lint:js -- --fix && npm run lint:style -- --fix",
    "prepare": "husky install",
    "postinstall": "nuxt prepare",
    "test": "vitest run",
    "lhci:mobile": "lhci autorun",
    "lhci:desktop": "lhci autorun --collect.settings.preset=desktop"
  },
  "lint-staged": {
    "*.{js,vue,ts}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@nuxt/content": "^2.13.0",
    "@nuxt/image": "^1.8.0",
    "core-js": "^3.40.0",
    "nuxt": "^4.0.0",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.7.1",
    "@lhci/cli": "^0.14.0",
    "@nuxt/eslint": "^0.5.0",
    "@nuxt/test-utils": "^3.14.0",
    "@nuxtjs/stylelint-module": "^5.2.0",
    "vite-svg-loader": "^5.1.0",
    "@nuxtjs/tailwindcss": "^6.13.2",
    "@vite-pwa/nuxt": "^0.10.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^10.4.0",
    "happy-dom": "^20.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.2",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "^1.86.2",
    "stylelint": "^16.14.1",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^39.0.0",
    "vitest": "^3.2.0"
  }
}
